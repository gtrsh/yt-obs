{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "watchOptions": {
    "preserveWatchOutput": true,
    "gracefulShutdown": true
  },
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "",
      "sourceRoot": "src",
      "entryFile": "main"
    },
    "worker": {
      "type": "application",
      "root": "",
      "sourceRoot": "src",
      "entryFile": "worker"
    }
  }
}
